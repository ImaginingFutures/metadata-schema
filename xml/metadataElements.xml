<?xml version="1.0" encoding="utf-8"?>
<profileName>Imagining Futures Through Un/Archived Pasts</profileName>
<profileDescription>Archive management for Projects and Collections</profileDescription>
<version>0.1-alpha</version>

<elementSets>
    <metadataElement code="idno" datatype="text">
        <labels>
            <label locale="en_US">
                <name>Unique Record Identifier</name>
                <description>An alphanumeric identifier for the record created for the IF
                    Repository.</description>
                <entry>String-CV. Derived from Projects ID</entry>
                <dcequilavent>dc:identifier</dcequilavent>
                <repeatable>No</repeatable>
                <modality>Mandatory</modality>
                <guideline>The PIs of each project will receive a pre-assigned unique project
                    identifier.
                    Unique
                    ID's derivered from that ID and adding 4 digit secuential numbers.</guideline>
                <example>UFC00800001</example>
            </label>
        </labels>
        <settings>
            <setting name="fieldWidth">90</setting>
            <setting name="fieldHeight">6</setting>
            <setting name="minChars">0</setting>
            <setting name="maxChars">65535</setting>
        </settings>
        <typeRestrictions>
            <restriction>
                <table>ca_objects</table>
                <settings>
                    <setting name="minAttributesPerRow">0</setting>
                    <setting name="maxAttributesPerRow">1</setting>
                    <setting name="minimumAttributeBundlesToDisplay">1</setting>
                </settings>
            </restriction>
        </typeRestrictions>
    </metadataElement>

